<odoo>
    <!-- Wizard Form View -->
    <record id="sale_wizard_view_form" model="ir.ui.view">
        <field name="name">Sale Order From Wizard</field>
        <field name="model">sale.wizard</field>
        <field name="arch" type="xml">
            <form string="Add Sale Order line">
                <group>
                     <field name="importfile"/>
                </group>
                <footer>
                    <button string="Import" type="object" name="action_open_wizard" class="btn-primary"/>
                    <button string="Cancel" special="cancel" class="btn-secondary"/>
                </footer>
            </form>
        </field>
    </record>


</odoo>








