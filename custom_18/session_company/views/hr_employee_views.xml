<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_employee_form_session_company" model="ir.ui.view">
        <field name="name">hr.employee.form.inherit.session.company</field>
        <field name="model">hr.employee</field>
        <field name="inherit_id" ref="hr.view_employee_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='coach_id']" position="after">
                <field name="login_company" invisible="1"/>
            </xpath>
        </field>
    </record>
</odoo>