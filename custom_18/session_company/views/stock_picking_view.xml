<?xml version="1.0"?>
<odoo>
    <!-- Model : stock.picking -->
    <!-- Picking Form Inherit -->
    <record id="view_picking_form_session_company" model="ir.ui.view">
        <field name="name">stock.picking.form.session.company</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='origin']" position="after">
                <field name="login_company" invisible="1"/> 
            </xpath>
        </field>
    </record>
</odoo>