<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="tests_model_view_form" model="ir.ui.view">
        <field name="name">tests.model.view.form</field>
        <field name="model">tests.model</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name" />
                        <field name="age" />
                    </group>

                </sheet>
            </form>
        </field>
    </record>

    <record id="action_tests_member" model="ir.actions.act_window">
        <field name="name">Teams</field>
        <field name="res_model">tests.model</field>
        <field name="view_mode">form</field>
    </record>

    <menuitem id="menu_tests_model" 
            name="Tests Model" 
            action="action_tests_member" 
            sequence="1"/>

</odoo>