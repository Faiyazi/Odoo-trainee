<?xml version="1.0" encoding="utf-8"?>
<odoo>

     <record id="project_team_add_member_server_action" model="ir.actions.server">
        <field name="name">Add Member</field>
        <field name="model_id" ref="model_project_team"/>
        <field name="state">code</field>
        <field name="binding_model_id" ref="model_project_team"/>
        <field name="binding_type">action</field>
        <field name="code">
            action = {
                'type': 'ir.actions.act_window',
                'res_model': 'add.member.wizard',
                'view_mode': 'form',
                'target': 'new',
                'context': {'active_ids': records.ids}
                }
        </field>
    </record>
</odoo>
